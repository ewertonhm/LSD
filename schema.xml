    <column name="hora" phpName="Hora" type="TIME" required="true"/>
    <column name="descricao" phpName="Descricao" type="LONGVARCHAR" required="true"/>
    <foreign-key foreignTable="trabalho" name="versao_codtrabalho_fkey">
      <reference local="codtrabalho" foreign="codtrabalho"/>
    </foreign-key>
  </table>
</database>