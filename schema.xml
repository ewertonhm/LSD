<?xml version="1.0" encoding="utf-8"?>
<database name="default" defaultIdMethod="native" namespace="App" defaultPhpNamingMethod="underscore">
  <table name="aluno" idMethod="native" phpName="Aluno" namespace="App">
    <column name="codaluno" phpName="Codaluno" type="INTEGER" primaryKey="true" autoIncrement="true" required="true"/>
    <column name="ra" phpName="Ra" type="INTEGER" required="true"/>
    <column name="nome" phpName="Nome" type="VARCHAR" size="50" required="true"/>
  </table>
  <table name="professor" idMethod="native" phpName="Professor" namespace="App">
    <column name="codprofessor" phpName="Codprofessor" type="INTEGER" primaryKey="true" autoIncrement="true" required="true"/>
    <column name="nome" phpName="Nome" type="VARCHAR" size="50" required="true"/>
  </table>
  <table name="trabalho" idMethod="native" phpName="Trabalho" namespace="App">
    <column name="codtrabalho" phpName="Codtrabalho" type="INTEGER" primaryKey="true" autoIncrement="true" required="true"/>
    <column name="nome" phpName="Nome" type="VARCHAR" size="50" required="true"/>
    <column name="codaluno" phpName="Codaluno" type="INTEGER" required="true"/>
    <column name="codprofessor" phpName="Codprofessor" type="INTEGER" required="true"/>
