    <foreign-key foreignTable="professor" name="trabalho_codprofessor_fkey">
      <reference local="codprofessor" foreign="codprofessor"/>
    </foreign-key>
  </table>
  <table name="versao" idMethod="native" phpName="Versao" namespace="App">
